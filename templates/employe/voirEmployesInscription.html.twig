{%  extends 'base.html.twig' %}

{% block title %}Formation Inscrit{% endblock %}
{% block body %}
    </br>
    {% if (message) %}
        {{ message }}
    {% endif %}
<div class="container" align="center">
<div class="shadow-lg p-3 mb-5 bg-body rounded" align="left">
<table class="table">
    <thead>
    <tr>
        <th scope="col">Nom du produit de la formation</th>
    </tr>
  </thead>
  <tbody>
{% for inscription in ensInscriptions %}
    <tr>
      <td>{{ inscription.formation.produit.libelle }}</td>
    </tr>
{% endfor %}
    </tbody>
</table>
</div>
</div>
{% endblock %}